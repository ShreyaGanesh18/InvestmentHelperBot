{
	"name": "Result_Investment_Intent",
        "description": "Recursively query the dataset",
        "conclusionStatement": {
          "messages": [
            {
              "content": "via code hook",
              "contentType": "PlainText"
            }
          ]
        },
        "fulfillmentActivity": {
          "codeHook": {
            "messageVersion": "1.0",
            "uri": "{{lambda-arn}}"
          },
          "type": "CodeHook"
        },
        "sampleUtterances": [
          "From my portfolio, how much have i invested in {particular_stock} at present?",
          "What is my current investment amount with {particular_stock}?",
          "How much have I invested in {particular_stock} according to my investment portfolio at the moment?"
        ],
    "slots": [
        {
            "slotType": "company_name", 
            "name": "particular_stock", 
            "slotConstraint": "Optional", 
            "valueElicitationPrompt": {
                "maxAttempts": 2, 
                "messages": [
                    {
                        "content": "particular stock", 
                        "contentType": "PlainText"
                    }
                ]
            }, 
            "priority": 1, 
            "slotTypeVersion": "$LATEST", 
            "sampleUtterances": [], 
            "description": "company name"
        }
    ]
}
